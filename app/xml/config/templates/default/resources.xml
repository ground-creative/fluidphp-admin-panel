	<block id="_tpl_check-email" _is_tpl="true">
		<css type="Page CSS">
			<resource>_tpl_page-auth-css</resource>
		</css>
	</block>
	
	<!-- 404 -->
	<block id="_tpl_404" _is_tpl="true">
