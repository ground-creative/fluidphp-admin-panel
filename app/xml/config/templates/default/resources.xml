			<resource>_tpl_page-auth-css</resource>
		</css>
		<js type="JS Page Level">
			<resource>_tpl_frm-login-js</resource>
			<resource>_tpl_ct-login-js</resource>
			<raw>window.onload = AW.start( 'Login' )</raw>
		</js>
	</block>
	
	<!-- Register Page -->
	<block id="_tpl_register" _is_tpl="true">
		<css type="Page CSS">
			<resource>_tpl_page-auth-css</resource>
		</css>
		<js type="JS Page Level">
			<resource>_tpl_frm-register-js</resource>
			<resource>_tpl_ct-register-js</resource>
			<raw>window.onload = AW.start( 'Register' )</raw>
		</js>
	</block>
	
	<!-- Forgot Password -->
	<block id="_tpl_forgot-password" _is_tpl="true">
		<css type="Page CSS">
			<resource>_tpl_page-auth-css</resource>
		</css>
		<js type="JS Page Level">
			<resource>_tpl_frm-forgot-pass-js</resource>
			<resource>_tpl_ct-forgot-pass-js</resource>-->
			<raw>window.onload = AW.start( 'ForgotPass' )</raw>
		</js>
