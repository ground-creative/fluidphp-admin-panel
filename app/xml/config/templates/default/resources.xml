	<!-- Globals -->
	<block id="_tpl_globals" _is_tpl="true">
		<css type="Icons. Uncomment required icon fonts">
			<resource>_tpl_boxicons-css</resource>
		</css>
		<css type="Core CSS">
			<resource>_tpl_core-css</resource>
			<resource>_tpl_theme-default-css</resource>
			<resource>_tpl_demo-css</resource>
			<resource>_tpl_perfect-scrollbar-css</resource>
			<resource>_tpl_overlay-loader-css</resource>
		</css>		
		<js type="Core JS">
			<resource>_tpl_jquery-js</resource>
			<resource>_tpl_popper-js</resource>
			<resource>_tpl_bootstrap-js</resource>
			<resource>_tpl_perfect-scroolbar-js</resource>
			<resource>_tpl_menu-js</resource>
		</js>
		<js type="Main JS">
			<resource>_tpl_main-js</resource>
			<resource>_tpl_aw-js</resource>
		</js>
	</block>
	
	<!-- Login Page -->
	<block id="_tpl_login" _is_tpl="true">
