<?xml version="1.0" encoding="UTF-8"?>
<package>
	
	<elements>
		<element name="_tpl_brand_logo" _is_tpl="true">templates/default/elements/_tpl_el.brand-logo</element>
		<element name="_tpl_top_bar" _is_tpl="true">templates/default/elements/_tpl_el.top-bar</element>
		<element name="_tpl_left_menu" _is_tpl="true">templates/default/elements/_tpl_el.left-menu</element>
		<element name="_tpl_footer" _is_tpl="true">templates/default/elements/_tpl_el.footer</element>
		<element name="_tpl_header" _is_tpl="true">templates/default/_header</element>
	</elements>
	
	<!-- Pages -->
	<page id="_tpl_index" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_skeleton</view>
			<view child="_content">vi.home</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
		</resources>
	</page>
	
	<page id="_tpl_login" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_login</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_login</block>
		</resources>
	</page>
	
	<page id="_tpl_register" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_register</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_register</block>
		</resources>
	</page>
		
	<page id="_tpl_forgot-password" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_forgot-password</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_forgot-password</block>
		</resources>
	</page>
	
	<page id="_tpl_check-email" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_check-email</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_check-email</block>
		</resources>
	</page>
		
	<page id="_tpl_reset-password" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_reset-password</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_reset-password</block>
		</resources>
	</page>
	
	<page id="_tpl_verify-account" callback="_Loader::verifyAccount" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_verify-account</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_check-email</block>
		</resources>
	</page>
	
	<page id="_tpl_404" _is_tpl="true">
		<views>
			<view>templates/default/_tpl_404</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
			<block>_tpl_404</block>
		</resources>
	</page>
	
	<page id="_tpl_privacy-policy" _is_tpl="true">
		<views>
			<view>privacy-policy</view>
		</views>
		<resources>
			<block>_tpl_globals</block>
		</resources>
	</page>

